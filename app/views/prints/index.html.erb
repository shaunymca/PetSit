        <legend>Monday</legend>
        <% @admin_visits.each do |visit| %>
          <% if visit.day_of_week == 'monday' %>
              <p>
              <% unless visit.checkin_date.nil? %>
                <div class='class="alert alert-success'>
                  <h4><%='<i class="icon-check"></i>'.html_safe + "  #{visit.time_text} #{visit.user.name}" %></h4>
                </div>
              <% else %>
                <h4><%="#{visit.time_text} - #{visit.user.name}"%></h4>
              <% end %>
              <%= link_to "#{visit.client.full_name} | #{visit.visit_type}", visit, 'data-no-turbolink' => true %>
              </p>
              <p>
              <%= visit.client.address_1 %>, <%= visit.client.address_2 %>
              <%= visit.client.city + ', ' + visit.client.state %>
