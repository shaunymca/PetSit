<%= simple_form_for(@visit) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :client_id %>
    <%= f.input :visit_type %>
    <%= f.input :visit_price %>
    <%= f.input :visit_date %>
    <%= f.input :invoice_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
